    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^0.6.2",
    "webpack": "^4.16.0",
    "webpack-bundle-tracker": "^0.2.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
